{
  "name": "nextra-theme-docs",
  "version": "4.0.0-app-router.35",
  "description": "A Nextra theme for documentation sites.",
  "repository": "https://github.com/shuding/nextra",
  "author": "Shu Ding <g@shud.in>",
  "license": "MIT",
  "exports": {
    "./style.css": "./dist/style.css",
    "./style-prefixed.css": "./dist/style-prefixed.css",
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch . --watch ../nextra/src --watch ../nextra/styles",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "types:check": "tsc --noEmit"
  },
  "peerDependencies": {
    "next": ">=14",
    "nextra": "workspace:*",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.2",
    "clsx": "^2.1.0",
    "next-themes": "^0.4.0",
    "react-compiler-runtime": "19.0.0-beta-df7b47d-20241124",
    "scroll-into-view-if-needed": "^3.1.0",
    "zod": "^3.22.3",
    "zod-validation-error": "^3.0.0",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.0-beta.2",
    "@testing-library/react": "^16.0.0",
    "@types/react": "^18.2.23",
    "@vitejs/plugin-react": "^4.1.0",
    "esbuild-react-compiler-plugin": "workspace:*",
    "jsdom": "^25.0.0",
    "next": "^15.0.2",
    "nextra": "workspace:*",
    "postcss": "^8.4.49",
    "react": "18.3.1",
    "tailwindcss": "^4.0.0-beta.2",
    "vitest": "^2.0.3"
  },
  "sideEffects": false
}
