{
  "name": "nextra",
  "version": "4.0.0-app-router.20",
  "type": "module",
  "description": "Next.js and MDX based site generator.",
  "repository": "https://github.com/shuding/nextra",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "exports": {
    "./katex.css": "./styles/katex.css",
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.js"
    },
    "./components": {
      "types": "./dist/client/components/index.d.ts",
      "import": "./dist/client/components/index.js"
    },
    "./hooks": {
      "types": "./dist/client/hooks/index.d.ts",
      "import": "./dist/client/hooks/index.js"
    },
    "./icons": {
      "types": "./dist/client/icons/index.d.ts",
      "import": "./dist/client/icons/index.js"
    },
    "./compile": {
      "types": "./dist/server/compile.d.ts",
      "import": "./dist/server/compile.js"
    },
    "./locales": {
      "types": "./dist/server/locales.d.ts",
      "import": "./dist/server/locales.js"
    },
    "./fetch-filepaths-from-github": {
      "types": "./dist/server/fetch-filepaths-from-github.d.ts",
      "import": "./dist/server/fetch-filepaths-from-github.js"
    },
    "./page-map": {
      "types": "./dist/server/page-map/index.d.ts",
      "import": "./dist/server/page-map/index.js"
    },
    "./schemas": {
      "types": "./dist/server/schemas.d.ts",
      "import": "./dist/server/schemas.js"
    },
    "./*": {
      "types": "./dist/client/*.d.ts",
      "import": "./dist/client/*.js"
    }
  },
  "types": "./dist/server/index.d.ts",
  "files": [
    "dist",
    "loader.cjs",
    "styles"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "NODE_OPTIONS=--max_old_space_size=8192 tsup --watch",
    "prepublishOnly": "pnpm build",
    "test": "vitest",
    "types": "tsup --dts-only",
    "types:check": "tsc --noEmit"
  },
  "peerDependencies": {
    "next": ">=14",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.5.4",
    "@headlessui/react": "^2.1.2",
    "@mdx-js/mdx": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@napi-rs/simple-git": "^0.1.9",
    "@shikijs/twoslash": "^1.0.0",
    "@theguild/remark-mermaid": "^0.1.3",
    "@theguild/remark-npm2yarn": "^0.3.2",
    "better-react-mathjax": "^2.0.3",
    "clsx": "^2.1.0",
    "estree-util-to-js": "^2.0.0",
    "estree-util-value-to-estree": "^3.0.1",
    "fast-glob": "^3.3.2",
    "github-slugger": "^2.0.0",
    "hast-util-to-estree": "^3.1.0",
    "katex": "^0.16.9",
    "mdast-util-from-markdown": "^2.0.1",
    "mdast-util-gfm": "^3.0.0",
    "mdast-util-to-hast": "^13.2.0",
    "negotiator": "^1.0.0",
    "rehype-katex": "^7.0.0",
    "rehype-pretty-code": "0.14.0",
    "rehype-raw": "^7.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "remark-reading-time": "^2.0.1",
    "remark-smartypants": "^3.0.0",
    "shiki": "^1.0.0",
    "slash": "^5.1.0",
    "title": "^3.5.3",
    "unist-util-remove": "^4.0.0",
    "unist-util-visit": "^5.0.0",
    "yaml": "^2.3.2",
    "zod": "^3.22.3",
    "zod-validation-error": "^3.0.0"
  },
  "devDependencies": {
    "@svgr/babel-plugin-remove-jsx-attribute": "^8.0.0",
    "@testing-library/react": "^16.0.0",
    "@types/estree": "^1.0.5",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/negotiator": "^0.6.3",
    "@types/react": "^18.3.3",
    "@types/webpack": "^5.28.5",
    "@vitejs/plugin-react": "^4.3.1",
    "esbuild-plugin-svgr": "^3.0.0",
    "next": "^15.0.2",
    "react": "18.3.1",
    "unified": "^11.0.5",
    "vitest": "^2.0.4"
  },
  "sideEffects": false
}
