{
  "name": "nextra",
  "version": "3.0.0-alpha.0",
  "description": "Next.js and MDX based site generator.",
  "repository": "https://github.com/shuding/nextra",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/server/index.js",
    "./loader": "./loader.cjs",
    "./components": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./mdx-plugins": {
      "import": "./dist/server/mdx-plugins/index.js",
      "types": "./dist/server/mdx-plugins/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    },
    "./icons": {
      "import": "./dist/icons/index.js",
      "types": "./dist/icons/index.d.ts"
    },
    "./compile": {
      "import": "./dist/server/compile.js",
      "types": "./dist/server/compile.d.ts"
    },
    "./remote": {
      "import": "./dist/server/remote.js",
      "types": "./dist/server/remote.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "types": "./dist/server/index.d.ts",
  "typesVersions": {
    "*": {
      "compile": [
        "./dist/server/compile.d.ts"
      ],
      "mdx-plugins": [
        "./dist/server/mdx-plugins/index.d.ts"
      ],
      "remote": [
        "./dist/server/remote.d.ts"
      ],
      "catch-all": [
        "./dist/catch-all.d.ts"
      ],
      "context": [
        "./dist/context.d.ts"
      ],
      "data": [
        "./dist/data.d.ts"
      ],
      "icons": [
        "./dist/icons/index.d.ts"
      ],
      "components": [
        "./dist/components/index.d.ts"
      ],
      "hooks": [
        "./dist/hooks/index.d.ts"
      ],
      "mdx": [
        "./dist/mdx.d.ts"
      ],
      "schemas": [
        "./dist/schemas.d.ts"
      ],
      "normalize-pages": [
        "./dist/normalize-pages.d.ts"
      ]
    }
  },
  "files": [
    "dist/*",
    "loader.cjs",
    "styles/*"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf ./dist ./style.css",
    "dev": "tsup --watch",
    "prepublishOnly": "pnpm build",
    "test": "vitest",
    "types": "tsup --dts-only",
    "types:check": "tsc --noEmit"
  },
  "peerDependencies": {
    "next": ">=13",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.10",
    "@mdx-js/mdx": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@napi-rs/simple-git": "^0.1.9",
    "@theguild/remark-mermaid": "^0.0.4",
    "@theguild/remark-npm2yarn": "0.2.0-alpha-20230904225308-5ba1c7b",
    "clsx": "^2.0.0",
    "estree-util-to-js": "^2.0.0",
    "estree-util-value-to-estree": "^3.0.1",
    "github-slugger": "^2.0.0",
    "graceful-fs": "^4.2.11",
    "gray-matter": "^4.0.3",
    "katex": "^0.16.8",
    "next-mdx-remote": "^4.2.1",
    "p-limit": "^4.0.0",
    "rehype-katex": "^6.0.3",
    "rehype-pretty-code": "0.9.11",
    "rehype-raw": "^7.0.0",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remark-reading-time": "^2.0.1",
    "shiki": "^0.14.3",
    "slash": "^5.1.0",
    "title": "^3.5.3",
    "unist-util-remove": "^4.0.0",
    "unist-util-visit": "^5.0.0",
    "yaml": "^2.3.2",
    "zod": "^3.22.2",
    "zod-validation-error": "^1.5.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/estree": "^1.0.1",
    "@types/graceful-fs": "^4.1.6",
    "@types/mdast": "^3.0.12",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/webpack": "^5.28.2",
    "@vitejs/plugin-react": "^3.0.1",
    "fast-glob": "^3.2.12",
    "next": "^13.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "unified": "^10.1.2",
    "vitest": "^0.34.0"
  },
  "sideEffects": false
}
