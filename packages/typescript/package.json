{
  "name": "@nextra/typescript",
  "version": "0.0.0",
  "description": "Typescript Integration for Nextra",
  "keywords": [
    "next.js",
    "nextra",
    "docs",
    "typescript"
  ],
  "homepage": "https://nextra.site",
  "repository": "github:shuding/nextra",
  "license": "MIT",
  "author": "Dimitri POSTOLOV",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./ui": {
      "import": "./dist/ui/index.js",
      "types": "./dist/ui/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "estree-util-value-to-estree": "^3.3.2",
    "fast-glob": "^3.3.3",
    "hast-util-to-estree": "^3.1.3",
    "hast-util-to-jsx-runtime": "^2.3.6",
    "remark": "^15.0.1",
    "remark-rehype": "^11.1.1",
    "shiki": "^3.2.1",
    "ts-morph": "^25.0.1",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^3.1.0",
    "@types/estree": "^1.0.6",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.3",
    "@types/node": "22.13.10",
    "@types/react": "19.0.11",
    "@types/react-dom": "19.0.4",
    "fumadocs-core": "^15.1.1",
    "fumadocs-ui": "^15.1.1",
    "@tsconfig/strictest": "^2.0.5",
    "typescript": "^5.8.2",
    "unified": "^11.0.5"
  },
  "peerDependencies": {
    "typescript": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
