{
  "extends": ["@tsconfig/strictest/tsconfig.json"],
  "compilerOptions": {
    "paths": {
      "@/*": ["./src/*"]
    },
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "target": "es2022",
    "module": "esnext",
    "noPropertyAccessFromIndexSignature": false
  },
  "exclude": ["node_modules", "dist"]
}
